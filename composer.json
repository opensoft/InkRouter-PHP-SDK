{
    "name": "opensoft/inkrouter-php-sdk",
    "description": "Small sdk for connecting to OpenSoft Job Submission Interface",
    "authors": [
        {
            "name": "Opensoft",
            "email": "info@opensoftdev.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "InkRouter\\": "src",
            "InkRouter\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-soap": "*",
        "ext-dom": "*",
        "ext-xmlwriter": "*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.5"
    }
}
